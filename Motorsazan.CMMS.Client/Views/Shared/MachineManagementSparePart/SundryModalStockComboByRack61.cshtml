    }).BindList(Motorsazan.CMMS.Client.Models.ViewModels.PagedStocks.GetFinalStockListByStoreCodeForSparePart61, Motorsazan.CMMS.Client.Models.ViewModels.PagedStocks.GetFinalMachineManagementSparePartCodeRangeById).GetHtml()