@{
    ViewBag.Title = "مدیریت گروه های تعمیراتی";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section head
{
    <link href="~/DX.ashx?cssfile=~/Views/maintenanceGroup/Css/Index.min.css" rel="Stylesheet" />
}
@section scripts
{
    <script src="~/DX.ashx?jsfile=~/Views/maintenanceGroup/JS/Index.js"></script>
}

<div class="row">
    <div class="col-xs-12 col-md-12">
        <section class="app__section">
            <div class="app__section__header">
                @ViewBag.Title
                @{ Html.RenderPartial("~/Views/Shared/Partials/_AppSectionSettings.cshtml"); }
            </div>
            <div class="app__section__body">
                <form class="app__grid-wrapper" id="maintenanceGroupGirdParent">
                    @{ Html.RenderAction("MaintenanceGroupGird"); }
                </form>
            </div>
        </section>
    </div>
</div>
